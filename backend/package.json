{
  "name": "nhis-fraud-auditor-backend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "node --loader ts-node/esm --experimental-specifier-resolution=node index.ts",
    "dev:watch": "nodemon --watch . --ext ts,js,json --exec \"node --loader ts-node/esm --experimental-specifier-resolution=node index.ts\"",
    "ingest": "node --loader ts-node/esm --experimental-specifier-resolution=node utils/ingest.ts data/claims.sample.csv",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run"
  },
  "dependencies": {
    "csv-parse": "^5.5.6",
    "express": "^5.1.0",
    "multer": "^1.4.5-lts.1",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.19.24",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.9"
  }
}
